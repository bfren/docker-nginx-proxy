{
    "$schema": "https://raw.githubusercontent.com/bfren/docker-nginx-proxy/main/ssl-conf-schema.json",
    "domains": [
        {
            "primary": "<%= ${PRIMARY} %>",
            "upstream": "<%= ${UPSTREAM} %>"<% if [ -n "${ALIASES}" ] ; then %>,
            "aliases": [ "<%= ${ALIASES} %>" ]<% fi ; if [ "${CUSTOM}" = "1" ] ; then %>,
            "custom": true<% fi %>
        }
    ]
}
